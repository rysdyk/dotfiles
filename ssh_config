Host *
	ServerAliveInterval 120

Host 10.10.10.*
	ServerAliveInterval 3600

Host apps1
	user xspond
	ProxyCommand ssh gateway@proxy1.xspond.com exec nc %h %p

Host apps2
	user xspond
	ProxyCommand ssh gateway@proxy1.xspond.com exec nc %h %p

Host db1
	user xspond
	ProxyCommand ssh gateway@proxy1.xspond.com exec nc %h %p

Host db2
	user xspond
	ProxyCommand ssh gateway@proxy1.xspond.com exec nc %h %p

Host legacy1
	user xspond
	HostName legacy1.xspond.com

Host media
	user xspond
	HostName media.xspond.com

Host proxy1
	user xspond
	HostName proxy1.xspond.com

Host staging
	user xspond
	HostName staging.xspond.com

Host utilities
	user xspond
	ProxyCommand ssh gateway@proxy1.xspond.com exec nc %h %p
